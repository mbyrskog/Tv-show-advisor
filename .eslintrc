{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "plugins": ["react", "react-hooks"],
    "rules": {
      "react/prop-types": "off"
    }
  }
  